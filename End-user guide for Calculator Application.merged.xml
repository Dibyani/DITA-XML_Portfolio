            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Choose the star(*) option.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Enter the second number.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Choose the equals(=) button.</cmd>
            </step>
        </steps>
        <result class="- topic/section task/result " id="unique_4_Connect_42_result_tqf_cxg_npb" nd:nd-id="result_tqf_cxg_npb">The result from multiplication is displayed.</result>
        <example class="- topic/example " id="unique_4_Connect_42_example_ajp_cxg_npb" nd:nd-id="example_ajp_cxg_npb">You want to multiply 2 with 3. The equation will look like
            3*2=6. You enter 3. Press star button. Enter 2. Press equals button.<p class="- topic/p ">The result 6
                appears on the screen.</p></example>
    </taskbody>
</task></topic><topic xmlns:ot-placeholder="http://suite-sol.com/namespaces/ot-placeholder" break-before="true" is-chapter="true" class="+ topic/topic pdf2-d/placeholder " id="d86e64" topicrefclass="+ map/topicref mapgroup-d/topichead "><title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">Divide</ph></title><task class="- topic/topic task/task " id="unique_5" topicrefclass="- map/topicref " nd:nd-id="task_omj_4tg_npb" oid="task_omj_4tg_npb" specializations="@props/deliveryTarget">
